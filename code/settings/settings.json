{
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": false,
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorSurroundingLines": 9,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "'MonoLisa', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": "'ss01', 'zero', 'liga' on, 'calt' on",
  "editor.fontSize": 13,
  "editor.fontWeight": "300",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.lineHeight": 28,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": true,
  "editor.padding.bottom": 13,
  "editor.padding.top": 13,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "none",
  "editor.rulers": [100, 120],
  "editor.semanticHighlighting.enabled": true,
  "editor.stablePeek": false,
  "editor.suggest.localityBonus": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 8,
  "editor.wordWrapColumn": 100,

  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "yaml": "dockercompose",
    "html": "nunjucks"
  },

  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,

  "npm-intellisense.importLinebreak": ";\n",
  "path-intellisense.autoSlashAfterDirectory": true,
  "window.openFoldersInNewWindow": "off",
  "window.restoreFullscreen": true,
  "workbench.colorTheme": "Material Theme",
  "workbench.iconTheme": "eq-material-theme-icons",
  "workbench.startupEditor": "none",
  "projectManager.showProjectNameInStatusBar": false,
  "extensions.ignoreRecommendations": true,
  "auto-close-tag.excludedTags": [
    "area",
    "base",
    "br",
    "col",
    "command",
    "embed",
    "hr",
    "img",
    "input",
    "keygen",
    "link",
    "meta",
    "param",
    "source",
    "track",
    "wbr",
    "Image"
  ],
  "auto-close-tag.enableAutoCloseSelfClosingTag": false,
  "search.useIgnoreFiles": false,
  "subtleBrackets.style": {
    "borderWidth": "1px",
    "borderStyle": "solid",
    "borderColor": "#F0F00F"
  },
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.keymap": "none",
  "javascript.validate.enable": false,
  "problems.decorations.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false
  },
  "files.associations": {
    "*.conf": "shellscript",
    "zshrc": "shellscript",
    "*.html": "html",
    "*.json5": "json"
  },
  "explorer.decorations.badges": false,
  "projectManager.groupList": true,
  "projectManager.sortList": "Recent",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "todo.embedded.exclude": [
    ".git",
    "**/node_modules/**",
    "**/flow-typed/**",
    "**/android/**",
    "**/ios/**"
  ],
  "todo.embedded.provider": "rg",
  "todo.embedded.providers.rg.regex": "(?:#|//|/\\*+|<!--|--|\\* @) *(TODO|FIXME|NOTE)",
  "todo.embedded.providers.rg.args": ["--ignore-case"],
  "highlight.regexes": {
    "((?:<!-- *)?(?://|/\\*+|<!--|--|\\* @) *TODO(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/|(?= *(?:[^:]//|/\\*+|<!--|@|--))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)))|(?: +[^@\\n]+)?))": [
      {
        "backgroundColor": "#FDFA7A",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#FDFA7A",
        "color": "#1f1f1f"
      }
    ],
    "((?:<!-- *)?(?://|/\\*+|<!--|--|\\* @) *FIXME(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/|(?= *(?:[^:]//|/\\*+|<!--|@|--))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)))|(?: +[^@\\n]+)?))": [
      {
        "backgroundColor": "#F6476D",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#F6476D",
        "color": "#1f1f1f"
      }
    ]
  },
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.reportStyleChecksAsWarnings": false,
  "typescript.implementationsCodeLens.enabled": true,
  "zenMode.hideLineNumbers": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "liveshare.featureSet": "insiders",
  "workbench.editor.revealIfOpen": true,
  "search.exclude": {
    "**/.next": true,
    "**/build": true,
    "**/dist": true
  },
  "workbench.tree.indent": 14,
  "workbench.settings.useSplitJSON": true,
  "peacock.favoriteColors": [
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "peacock.elementAdjustments": {
    "activityBar": "none"
  },
  "peacock.affectActivityBar": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultDateStyle": "absolute",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.printWidth": 100,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "terminal.integrated.rendererType": "experimentalWebgl",
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Chrome - localhost:3000",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src"
      },
      {
        "name": "Chrome - localhost:3333",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3333",
        "webRoot": "${workspaceFolder}/src"
      },
      {
        "name": "CRA tests single run",
        "type": "node",
        "request": "launch",
        "env": { "CI": "true" },
        "runtimeExecutable": "${workspaceRoot}/frontend/node_modules/.bin/react-scripts",
        "args": ["test", "${fileBasenameNoExtension}", "--runInBand", "--no-cache"],
        "cwd": "${workspaceRoot}/frontend",
        "protocol": "inspector",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      },
      {
        "name": "Jest single run",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "args": ["${fileBasename}", "--verbose", "-i", "--no-cache"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  },
  "workbench.colorCustomizations": {
    // "errorLens.hintForeground": "#ffc600A1",
    // "errorLens.hintBackground": "#ffc60003",
    // "errorLens.errorBackground": "#ff000005"
    "[Material Theme]": {
      "editorLineNumber.foreground": "#65737E",
      "sideBar.foreground": "#677a88",
      "editorIndentGuide.activeBackground": "#f0f00fa2"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Material Theme]": {
      "comments": "#768b92"
    }
  },
  "workbench.view.experimental.allowMovingToNewContainer": true,
  "screencastMode.onlyKeyboardShortcuts": true,
  "search.showLineNumbers": true,
  "eslint.alwaysShowStatus": true,
  "search.smartCase": true,
  "[markdown]": {
    "editor.rulers": [120],
    "editor.wordWrapColumn": 120,
    "editor.wordWrap": "on",
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.suggestOnTriggerCharacters": false,
    "editor.formatOnSave": false,
    "editor.tabSize": 4
  },
  "[mdx]": {
    "editor.rulers": [120],
    "editor.wordWrapColumn": 120,
    "editor.wordWrap": "on",
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.suggestOnTriggerCharacters": false,
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.codeActionsOnSave": [
      "source.addMissingImports",
      "source.organizeImports",
      "source.fixAll.eslint"
    ]
  },
  "[plaintext]": {
    "editor.rulers": [],
    "editor.wordWrapColumn": 120,
    "editor.wordWrap": "off",
    "editor.quickSuggestions": {
      "other": false,
      "comments": false,
      "strings": false
    }
  },
  "[yaml]": {
    "editor.rulers": [120],
    "editor.wordWrapColumn": 120,
    "editor.wordWrap": "off"
  },
  "[dockercompose]": {
    "editor.rulers": [120],
    "editor.wordWrapColumn": 120,
    "editor.wordWrap": "off"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.addMissingImports",
      "source.organizeImports",
      "source.fixAll.stylelint",
      "source.fixAll.eslint"
    ]
  },
  "[scss]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.codeActionsOnSave": {
      "source.fixAll.stylelint": true
    }
  },
  "[css]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.stylelint": true
    }
  },
  "[nunjucks]": {
    "editor.defaultFormatter": "okitavera.vscode-nunjucks-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.addMissingImports",
      "source.organizeImports",
      "source.fixAll.eslint"
    ]
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.addMissingImports",
      "source.organizeImports",
      "source.fixAll.eslint"
    ]
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "zenMode.hideStatusBar": false,
  "materialTheme.accent": "Teal",
  "editor.stickyTabStops": true,
  "tabnine.experimentalAutoImports": true,
  "projectManager.git.baseFolders": ["~/WebDev", "~/Ataccama"],
  "subtleBrackets.disableNative": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.tree.renderIndentGuides": "always",
  "workbench.editor.labelFormat": "short",
  "errorLens.fontSize": "12",
  "errorLens.onSave": true,
  // "errorLens.exclude": ["never (read|used)"]
  "errorLens.exclude": ["deprecated"],
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "hint"],
  "workbench.editor.untitled.hint": "hidden",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.tabs.location": "left",
  "editor.wordWrap": "on",
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.textwidth": 100,
  "editor.lineNumbers": "relative",
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "workbench.editor.showTabs": false,
  "editor.quickSuggestions": {
    "comments": "off",
    "strings": "off",
    "other": "off"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.suggest.preview": true,
  "editor.inlineSuggest.enabled": false,
  "editor.snippetSuggestions": "top",
  "editor.inlineSuggest.mode": "prefix",
  "editor.suggest.showWords": false,
  "workbench.enableExperiments": false,
  "editor.guides.bracketPairs": true,
  "peacock.affectAccentBorders": false,
  "peacock.showColorInStatusBar": false,
  "eslint.debug": true,
  "editor.minimap.enabled": false,
  "redhat.telemetry.enabled": false,
  "typescript.workspaceSymbols.scope": "currentProject",
  "workbench.activityBar.visible": false,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "debug.javascript.autoAttachFilter": "disabled"
}
