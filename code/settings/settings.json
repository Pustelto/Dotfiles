{
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true,
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "editor.cursorBlinking": "phase",
  "editor.cursorSurroundingLines": 3,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 13,
  "editor.fontWeight": "400",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleReferences": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.lineHeight": 28,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": true,
  "editor.padding.bottom": 13,
  "editor.padding.top": 13,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "none",
  "editor.rulers": [100, 120],
  "editor.semanticHighlighting.enabled": true,
  "editor.stablePeek": false,
  "editor.suggest.localityBonus": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 100,

  "emmet.triggerExpansionOnTab": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "html": "nunjucks"
  },

  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,

  "npm-intellisense.importLinebreak": ";\n",
  "path-intellisense.autoSlashAfterDirectory": true,
  "window.openFoldersInNewWindow": "off",
  "window.restoreFullscreen": true,
  "workbench.colorTheme": "Material Theme",
  "workbench.iconTheme": "eq-material-theme-icons",
  "workbench.startupEditor": "none",
  "projectManager.showProjectNameInStatusBar": false,
  "extensions.ignoreRecommendations": false,
  "todohighlight.isEnable": true,
  "auto-close-tag.excludedTags": [
    "area",
    "base",
    "br",
    "col",
    "command",
    "embed",
    "hr",
    "img",
    "input",
    "keygen",
    "link",
    "meta",
    "param",
    "source",
    "track",
    "wbr",
    "Image"
  ],
  "workbench.activityBar.visible": true,
  "auto-close-tag.enableAutoCloseSelfClosingTag": false,
  "search.useIgnoreFiles": false,
  "subtleBrackets.style": {
    "borderWidth": "2px",
    "borderStyle": "none none solid none",
    "borderColor": "#F0F00F"
  },
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.keymap": "none",
  "javascript.validate.enable": false,
  "problems.decorations.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "files.associations": {
    "*.conf": "shellscript",
    "zshrc": "shellscript",
    "*.html": "html"
  },
  "gitlens.historyExplorer.enabled": true,
  "materialTheme.fixIconsRunning": false,
  "materialTheme.accentPrevious": "Bright Teal",
  "explorer.decorations.badges": false,
  "projectManager.groupList": true,
  "projectManager.sortList": "Recent",
  "importCost.mediumPackageColor": "#e9e522",
  "importCost.mediumPackageSize": 70,
  "importCost.smallPackageSize": 15,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "todo.embedded.exclude": [
    ".git",
    "**/node_modules/**",
    "**/flow-typed/**",
    "**/android/**",
    "**/ios/**"
  ],
  "todo.embedded.provider": "rg",
  "todo.embedded.providers.rg.regex": "(?:#|//|/\\*+|<!--|--|\\* @) *(TODO|FIXME|NOTE)",
  "todo.embedded.providers.rg.args": ["--ignore-case"],
  "highlight.regexes": {
    "((?:<!-- *)?(?://|/\\*+|<!--|--|\\* @) *TODO(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/|(?= *(?:[^:]//|/\\*+|<!--|@|--))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)))|(?: +[^@\\n]+)?))": [
      {
        "backgroundColor": "#ffcc00",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#d9ad00",
        "color": "#1f1f1f"
      }
    ],
    "((?:<!-- *)?(?://|/\\*+|<!--|--|\\* @) *FIXME(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/|(?= *(?:[^:]//|/\\*+|<!--|@|--))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)))|(?: +[^@\\n]+)?))": [
      {
        "backgroundColor": "#ff0000",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#d90000",
        "color": "#1f1f1f"
      }
    ]
  },
  "breadcrumbs.symbolSortOrder": "name",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.reportStyleChecksAsWarnings": false,
  "projectManager.treeview.visible": false,
  "typescript.implementationsCodeLens.enabled": true,
  "zenMode.hideLineNumbers": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "liveshare.featureSet": "insiders",
  "workbench.editor.revealIfOpen": true,
  "search.exclude": {
    "**/.next": true,
    "**/build": true,
    "**/dist": true
  },
  "workbench.tree.indent": 12,
  "workbench.editor.tabSizing": "shrink",
  "workbench.settings.useSplitJSON": true,
  "peacock.favoriteColors": [
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "peacock.elementAdjustments": {
    "activityBar": "none"
  },
  "peacock.affectActivityBar": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultDateStyle": "absolute",
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "html.renameOnType": true,
  "subtleBrackets.disableNative": false,
  "prettier.printWidth": 100,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "terminal.integrated.rendererType": "experimentalWebgl",
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Chrome - localhost:3000",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src"
      },
      {
        "name": "Chrome - localhost:3333",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3333",
        "webRoot": "${workspaceFolder}/src"
      },
      {
        "name": "CRA tests single run",
        "type": "node",
        "request": "launch",
        "env": { "CI": "true" },
        "runtimeExecutable": "${workspaceRoot}/frontend/node_modules/.bin/react-scripts",
        "args": ["test", "${fileBasenameNoExtension}", "--runInBand", "--no-cache"],
        "cwd": "${workspaceRoot}/frontend",
        "protocol": "inspector",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      },
      {
        "name": "Jest single run",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
        "args": ["${fileBasename}", "--verbose", "-i", "--no-cache"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  },
  "workbench.colorCustomizations": {
    "[Material Theme]": {
      "editorLineNumber.foreground": "#65737E",
      "sideBar.foreground": "#677a88"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Material Theme]": {
      "comments": "#768b92"
    }
  },
  "workbench.view.experimental.allowMovingToNewContainer": true,
  "screencastMode.onlyKeyboardShortcuts": true,
  "search.showLineNumbers": true,
  "eslint.alwaysShowStatus": true,
  "search.smartCase": true,
  "[markdown]": {
    "editor.rulers": [120],
    "editor.wordWrapColumn": 120,
    "editor.wordWrap": "bounded",
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestions": false,
    "editor.suggestOnTriggerCharacters": false,
    "editor.formatOnSave": false
  },
  "[plaintext]": {
    "editor.rulers": [],
    "editor.wordWrapColumn": 120,
    "editor.wordWrap": "off"
  },
  "[scss]": {
    "editor.quickSuggestions": false,
    "editor.codeActionsOnSave": {
      "source.fixAll.stylelint": true
    }
  },
  "[css]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.stylelint": true
    }
  },
  "[nunjucks]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.addMissingImports",
      "source.organizeImports",
      "source.fixAll.stylelint",
      "source.fixAll.eslint"
    ]
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.addMissingImports",
      "source.organizeImports",
      "source.fixAll.tslint",
      "source.fixAll.eslint"
    ]
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": [
      "source.addMissingImports",
      "source.organizeImports",
      "source.fixAll.tslint",
      "source.fixAll.eslint"
    ]
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "zenMode.hideStatusBar": false,
  "materialTheme.accent": "Teal",
  "editor.stickyTabStops": true,
  "tabnine.experimentalAutoImports": true,
  "window.zoomLevel": -1
}
